<Page
    x:Class="DexterityApp.Views.Common.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:properties="clr-namespace:DexterityApp.Properties"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800" Background="{StaticResource GradientBrush}" Loaded="HomePage_OnLoaded" >
  
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Image Source="../../Resources/large_logo.png"  Stretch="Uniform" HorizontalAlignment="Center" VerticalAlignment="Center" Width="500" Height="500"  />

    </Grid>
</Page>